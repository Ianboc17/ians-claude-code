{
  "name": "ians-claude-code",
  "version": "1.0.0",
  "description": "Ian's personal Claude Code setup with 14 productivity commands and 11 specialized AI agents for modern web development",
  "author": {
    "name": "Ian",
    "email": "[email protected]",
    "url": "https://github.com/Ianboc17"
  },
  "license": "MIT",
  "homepage": "https://github.com/Ianboc17/ians-claude-code",
  "repository": "https://github.com/Ianboc17/ians-claude-code",
  "keywords": [
    "productivity",
    "nextjs",
    "supabase",
    "typescript",
    "react",
    "api",
    "development",
    "mcp"
  ],
  "commands": [
    "./.claude/commands/",
    "./.claude/commands/api/",
    "./.claude/commands/ui/",
    "./.claude/commands/misc/",
    "./.claude/commands/supabase/"
  ],
  "agents": "./.claude/agents/",
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "description": "Access up-to-date documentation for any library"
    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "description": "Browser automation and web testing capabilities"
    },
    "supabase": {
      "command": "npx",
      "args": ["-y", "@supabase/mcp-server-supabase"],
      "description": "Supabase database operations and management"
    }
  }
}
